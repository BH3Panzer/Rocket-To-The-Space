<Window x:Name="mainWindow" x:Class="Rocket_To_The_Space.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rocket_To_The_Space"
        mc:Ignorable="d"
        Title="Rocket To The Space" Height="609" Width="520" ResizeMode="CanMinimize"
        TextOptions.TextRenderingMode="Aliased"
        RenderOptions.EdgeMode="Aliased"
        RenderOptions.BitmapScalingMode="NearestNeighbor"
        >
    <Canvas x:Name="mainCanvas">
        <ContentControl ClipToBounds="True">
            <ContentControl x:Name="mainContentControl"/>
        </ContentControl>
        <Image x:Name="cursor" Source="pack://application:,,,/Assets/Img/cursor.png" Stretch="Fill" Height="32" Canvas.Left="157" Canvas.Top="47" Width="32" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Canvas>
</Window>
