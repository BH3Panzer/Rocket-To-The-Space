<UserControl x:Class="Rocket_To_The_Space.UCGame"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Rocket_To_The_Space"
             mc:Ignorable="d" 
             d:DesignHeight="609" d:DesignWidth="520">
    <Canvas x:Name="gameCanvas" Width="520" Height="609" HorizontalAlignment="Left" VerticalAlignment="Top">
        <Image x:Name="bg" x:Uid="bg" Source="pack://application:,,,/Assets/Img/bg.png" Canvas.Left="0" Canvas.Top="-64965" StretchDirection="UpOnly" Stretch="Fill" VerticalAlignment="Top" HorizontalAlignment="Center" MinWidth="520" Width="360"/>
        <Button x:Name="launchButton" Content="Launch" Canvas.Left="206" Canvas.Top="10" HorizontalAlignment="Left" VerticalAlignment="Center" Panel.ZIndex="3"/>
        <Label Content="stage 2" x:Uid="stage2" Canvas.Left="265" Canvas.Top="-5316" FontSize="100"/>
        <Label Content="stage 3" x:Uid="stage3" Canvas.Left="246" Canvas.Top="-29858" FontSize="100"/>
        <Label Content="stage 4" x:Uid="stage4" Canvas.Left="265" Canvas.Top="-45008" FontSize="100"/>
        <Label Content="stage 5" x:Uid="stage5" Canvas.Left="236" Canvas.Top="-49590" FontSize="100"/>
        <Label Content="stage 6" x:Uid="stage6" Canvas.Left="227" Canvas.Top="-62820" FontSize="100"/>
        <Label Content="stage end" x:Uid="stageEnd" Canvas.Left="227" Canvas.Top="-64965" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="100"/>
        <Image x:Name="launchpad" Height="528" Source="pack://application:,,,/Assets/Img/starting_point.png" Stretch="Fill" Canvas.Top="43" Width="520" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="-2" Panel.ZIndex="2"/>
        <Grid x:Name="shop" Canvas.Left="368" Canvas.Top="353" Height="172" Width="150" HorizontalAlignment="Left" VerticalAlignment="Top" Panel.ZIndex="3">
            <Rectangle x:Name="shopBackground" Height="175" Canvas.Left="394" Stroke="White" Fill="White" Canvas.Top="408" Width="150" HorizontalAlignment="Center" VerticalAlignment="Top" Panel.ZIndex="3"/>
            <Label x:Name="shopTitle" Content="Shop" HorizontalAlignment="Center" VerticalAlignment="Top" Panel.ZIndex="3"/>
            <WrapPanel>
                
            </WrapPanel>
        </Grid>
    </Canvas>
</UserControl>
